class Human:
    def __init__(self, name, age, gender):
        self.name = name
        self.age = age
        self.gender = gender

    def check_adult(self):
        if self.age >= 20:
            return  self.name +"さんは"+str(self.age)+"歳の"+self.gender+"でもう成年です。"
        else:
            return  self.name + "さんは"+str(self.age)+"歳の"+self.gender+"でまだ未成年です。"

A = Human("A", 19, "男性")
B = Human("B", 20, "女性")
C = Human("C", 18, "男性")
D = Human("D", 24, "女性")
E = Human("E", 21, "男性")

print(A.check_adult())
print(B.check_adult())
print(C.check_adult())
print(D.check_adult())
print(E.check_adult())
